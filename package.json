{
  "name": "mumii-monorepo",
  "version": "0.1.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "services/*"
  ],
  "scripts": {
    "dev": "npm run dev --workspace=apps/frontend",
    "build": "npm run build --workspace=apps/frontend",
    "start": "npm run start --workspace=apps/frontend",
    "lint": "npm run lint --workspace=apps/frontend",
    "dev:frontend": "npm run dev --workspace=apps/frontend",
    "build:frontend": "npm run build --workspace=apps/frontend",
    "dev:api": "npm run dev --workspace=services/api-service",
    "build:api": "npm run build --workspace=services/api-service",
    "install:all": "npm install && npm install --workspaces"
  },
  "devDependencies": {
    "@types/node": "^20"
  },
  "dependencies": {
    "next": "^15.4.5",
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  }
}
